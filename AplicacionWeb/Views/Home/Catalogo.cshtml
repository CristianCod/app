
@{
    ViewBag.Title = "Catalogo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="novedades" >
    <!--Comienzo Columnas-->
    <h1>Catálogo</h1>
    <div id="articulos"><!--sql productos-->
        @if (ViewBag.Productos != null)
        {
            foreach (AplicacionWeb.Models.Productos producto in ViewBag.Productos)
            {

                <div class="row featurette">
                    <!--verificar datos-->
                    <div class="col-md-8 col-md-push-4">
                        <h2 class="featurette-heading">@producto.Titulo <span class="text-muted">Compralo ahora por $@producto.Precio</span></h2>
                        <p class="lead">
                            @producto.Info
                        </p>
                        <br>
                        <button class="btn btn-default" type="button"><a href="#">Comprar</a></button>
                        <button class="btn btn-block" type="button"> <a href=@Url.Action("compraCatalogo", "Home")></a>Agregar a Compras</button>
                    </div>
                    <br>
                    <div class="col-md-4 col-md-pull-8">
                        <img class="featurette-image img-responsive center-block" data-src="holder.js/500x500/auto" alt="500x500" src="@producto.Imagen" data-holder-rendered="true">
                    </div>
                </div>

                <hr class="featurette-divider">

                <!--Verificacion de row al otro lado-->

                <div class="row featurette">
                    <div class="col-md-8 col-md-push-4">
                        <h2 class="featurette-heading">@producto.Titulo <span class="text-muted">Compralo ahora por $.</span></h2>
                        <p class="lead">@producto.Info
                        </p>

                        <br>
                        <button class="btn btn-default" type="button"><a href="#">Comprar</a></button>
                    <button class="btn btn-block" type="button">Agregar a Compras</button>
                    </div>
                    <br>
                    <div class="col-md-4 col-md-pull-8">
                        <img class="featurette-image img-responsive center-block" data-src="holder.js/500x500/auto" alt="500x500" src="@producto.Imagen" data-holder-rendered="true">
                    </div>
                </div>
                <hr class="featurette-divider">


                <!--fin verificacion-->
            }
        }
    </div><!--fin sql productos-->

